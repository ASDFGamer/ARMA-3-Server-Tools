      <Button fx:id="btnShops" layoutX="125.0" layoutY="333.0" mnemonicParsing="false" onAction="#handleBtnShopsAction" text="Shops">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
    </children>
</AnchorPane>
