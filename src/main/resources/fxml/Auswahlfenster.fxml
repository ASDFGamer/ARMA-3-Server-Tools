      <Label fx:id="lblTitle" layoutX="218.0" layoutY="14.0" text="ARMA 3 Server Tools">
         <font>
            <Font size="22.0" />
         </font>
      </Label>
      <Button fx:id="btnPlayerManager" layoutX="87.0" layoutY="109.0" mnemonicParsing="false" onAction="#handleBtnPlayerManagerAction" style="-fx-background-color: yellow;" text="Player Manager">
